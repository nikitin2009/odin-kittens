<%= form_for @kitten do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <%= f.label :cuteness %>
  <%= f.text_field :cuteness %>
  <%= f.label :softness %>
  <%= f.text_field :softness %>
  <%= f.submit %>
<% end %>